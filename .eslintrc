{
    "parser": "babel-eslint",
    "plugins": ["react"],
    "rules": {
        "no-unused-vars": 0,
        "max-len": [1, 100, 2, { "ignoreComments": true }],
        "linebreak-style": 0,
        "semi": "off",
        "indent": ["error", 4],
        "no-console": 0,
        "jsx-quotes": 1
    },
    "globals": {
        "window": true,
        "document": true
    },
    "extends": ["airbnb-base"]
}
